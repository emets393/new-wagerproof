{
  "expo": {
    "name": "WagerProof",
    "slug": "wagerproof-mobile",
    "version": "3.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": [
      "wagerproof",
      "rc-ff2fe0e0af"
    ],
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wagerproof.mobile",
      "buildNumber": "23",
      "usesAppleSignIn": true,
      "appleTeamId": "88DXY6L653",
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.wagerproof.mobile"
        ]
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#22c55e"
      },
      "package": "com.wagerproof.mobile",
      "versionCode": 33,
      "permissions": [
        "com.google.android.gms.permission.AD_ID"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.142325632215-agrfdkh87j01kgfa4uv4opuohl5l01lq"
        }
      ],
      "expo-apple-authentication",
      "@bacons/apple-targets",
      "./plugins/withWidgetDataBridge",
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "WagerProof uses this to improve analytics and personalize your experience. Your data helps us provide better predictions and insights."
        }
      ],
      [
        "expo-speech-recognition",
        {
          "microphonePermission": "WagerProof needs microphone access for voice-powered Roast Mode.",
          "speechRecognitionPermission": "WagerProof needs speech recognition for voice-powered Roast Mode."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e00a12fb-670d-4d36-87f4-ae8c63d715d5"
      }
    },
    "owner": "chrishabib"
  }
}
